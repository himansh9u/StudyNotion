{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/course/(.*)",
            "dest": "./routes/Course.js"
        },
        {
            "src": "/(.*)",
            "dest": "./index.js"
        }
    ]
}